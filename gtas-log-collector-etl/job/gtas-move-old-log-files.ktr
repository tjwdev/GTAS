        <class_source>
import java.util.*;
private Calendar calendar;
private Calendar calendarMod;


public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    

	String modYear = null;
	String modMonth = null;
	String modDay = null;

	// First, get a row from the default input hop
	//
	Object[] r = getRow();

    // If the row object is null, we are done processing.
	//
	if (r == null) {
		setOutputDone();
		return false;
	}

	

    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
	//
    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());

	String deleteDateStr = get(Fields.In, "deleteDateVar").getString(r);
	String lastModDateStr = get(Fields.In, "lastModDate").getString(r);


	calendar = Calendar.getInstance();
	Date deleteDtm = null;
	Date fileModifiedDate = null;

	if(deleteDateStr!=null &amp;&amp; !deleteDateStr.trim().isEmpty())
	{

			int deleteDateInt = Integer.parseInt(deleteDateStr);
			deleteDateInt = deleteDateInt * -1;
		
			calendar.add(Calendar.DATE, deleteDateInt);
			calendar.set(Calendar.HOUR_OF_DAY, 0);  
        	calendar.set(Calendar.MINUTE, 0);  
        	calendar.set(Calendar.SECOND, 0);  
       		calendar.set(Calendar.MILLISECOND, 0);  
				 
				
			deleteDtm = calendar.getTime();
			

			logDebug("### BOUNDARY YEAR = " + calendar.get(Calendar.YEAR));
			logDebug("### BOUNDARY MONTH = " + calendar.get(Calendar.MONTH));
			logDebug("### BOUNDARY DAY = " + calendar.get(Calendar.DATE));

			
	}
	
	else
	{
		logError("### ERROR! The deleteFilesBefore Date is null or empty or not in YYYY-MM-DD format");
		setOutputDone();
		return false;

	}


	calendarMod = Calendar.getInstance();

	if(lastModDateStr!=null &amp;&amp; !lastModDateStr.trim().isEmpty())
	{

			modYear = lastModDateStr.substring(0,4);
			int modYearInt = Integer.parseInt(modYear);
			modMonth =  lastModDateStr.substring(5,7);
			int modMonthInt = Integer.parseInt(modMonth)-1;
			modDay =  lastModDateStr.substring(8);
			int modDayInt  = Integer.parseInt(modDay);

			//logDebug("### YEAR = " + modYear);
			//logDebug("### MONTH = " + modMonth);
			//logDebug("### DAY = " + modDay);

			//delete  date
			calendarMod.set(Calendar.YEAR, modYearInt );
			calendarMod.set(Calendar.MONTH, modMonthInt);
			calendarMod.set(Calendar.DATE, modDayInt);
			calendarMod.set(Calendar.HOUR_OF_DAY, 0);  
        	calendarMod.set(Calendar.MINUTE, 0);  
        	calendarMod.set(Calendar.SECOND, 0);  
       		calendarMod.set(Calendar.MILLISECOND, 0);  
			fileModifiedDate = calendarMod.getTime();
				
			
	}
	
	else
	{
		logError("### ERROR! The lastModDateStr Date is null or empty or not in YYYY-MM-DD format");
		setOutputDone();
		return false;

	}


	
	// Set the value in the output field
	//
	boolean deleteFlag = calendarMod.before(calendar);
	calendarMod.clear();
	calendar.clear();

	get(Fields.Out, "deleteFlag").setValue(outputRow, deleteFlag);
	get(Fields.Out, "deleteDtm").setValue(outputRow, deleteDtm);
	get(Fields.Out, "fileModifiedDate").setValue(outputRow, fileModifiedDate);


    
    // putRow will send the row on to the default output hop.
	//
    putRow(data.outputRowMeta, outputRow);

	return true;
}</class_source>
      </definition>
